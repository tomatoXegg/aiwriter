{
  "name": "aiwriter-backend",
  "version": "1.0.0",
  "description": "AI Writer Backend Service",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "ts-node src/database/migrations/cli.ts",
    "seed": "ts-node src/database/seeds/cli.ts",
    "db:init": "ts-node src/database/init.ts",
    "db:status": "npm run migrate status",
    "db:up": "npm run migrate up",
    "db:down": "npm run migrate down",
    "db:create": "npm run migrate create",
    "db:seed": "npm run seed run",
    "db:seed:dev": "npm run seed run development-data --clear --verbose",
    "db:seed:list": "npm run seed list",
    "db:seed:create": "npm run seed create"
  },
  "keywords": ["ai", "writer", "backend", "express"],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "sqlite3": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "@google/generative-ai": "^0.2.1",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "fs-extra": "^11.1.1",
    "mammoth": "^1.6.0",
    "pdf-parse": "^1.1.1",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^6.0.2",
    "@types/node-cache": "^4.2.5",
    "typescript": "^5.3.3",
    "ts-jest": "^29.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.1"
  }
}
